all: singthreads.so

clean:
	rm -f singthreads.o singthreads.so

singthreads.o: singthreads.cc
	@CXX@ @CXXFLAGS@ @SING_INCLUDES@ @SHOBJ_CFLAGS@ -c singthreads.cc

singthreads.so: singthreads.o
	@CXX@ @LDFLAGS@ @SHOBJ_LDFLAGS@ -o singthreads.so singthreads.o

install: singthreads.so
@INSTALLATION@
.PHONY: all clean install
